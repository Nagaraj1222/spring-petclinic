pipeline
{
agent any
tools{
maven 'maven'
}
stages{
stage('Git checkout'){
steps{
git branch: 'feature/2025.03.10', url: 'https://github.com/parasa7358/onlinebookstore.git'
}
}
stage('clean and install'){
steps{
sh 'mvn clean install'
}
}
stage('Package'){
steps{
sh 'mvn package'
}
}
stage('Archive the Artifacts'){
steps{
sh 'mvn clean install'
}
post{
success{
archiveArtifacts artifacts: '**target/*.war'
}
}
}

stage('Test Cases'){
steps{
sh 'mvn test'
}
}
}
}